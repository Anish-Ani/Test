@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script>
        function process() {
            var name = $('#fname').val();

            $.ajax({
                type: "GET",
                url: "getData",
                data: { data: name },
                dataType: "text",
                success: function (resultData) {
                    alert("Save Complete " + resultData);
                    value = 10;
                    for (var i = 1; i <=10; i++)
                    {
                        rowCreation(i,value)
                    }
                }
            });
        }
        function rowCreation(i, value) {
            var sum = 0;
            for (var j = 1; j <=value; j++) {
                console.log(i * j)
                var ans = i * j;
                sum += ans;
            }
            console.log(sum);
            console.log(sum / value);
            console.log("=======================")
        }
    </script>
</head>
<body>
  

    <div>
        <h1>Home Page<h1>
                <input type="text" id="fname" name="fname">
                <input type="submit" value="submit" onclick="process()">
    </div>

</body>
</html>
